{
  "kind": "collectionType",
  "collectionName": "messages",
  "info": {
    "name": "Message"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "default",
        "unassigned",
        "review",
        "published",
        "feedback"
      ]
    },
    "feedback": {
      "type": "enumeration",
      "enum": [
        "default",
        "positive",
        "negative"
      ]
    },
    "media": {
      "type": "component",
      "repeatable": true,
      "component": "media.media-item"
    }
  }
}
